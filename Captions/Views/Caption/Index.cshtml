@{
    ViewBag.Title = "Captions Page";
}
@using Captions.Models
@model IEnumerable<Caption>

@{
    ViewBag.Title = "Index";
}

@foreach (var item in Model)
{
    if (item.ContentType == "image/jpeg")
    {
        <h2>@item.Title</h2>
        <img src="@Url.Action("GetImage", new { id = item.ID })" alt="Image" />

    }
}
